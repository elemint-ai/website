{
  "name": "code-like-google",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next export",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write .",
    "test-all": "npm run check-format && npm run check-lint && npm run check-types && npm run build",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && rm -rf .next/ && npm install",
    "prepare": "husky install"
  },
  "dependencies": {
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "babel-plugin-superjson-next": "^0.3.0",
    "classnames": "2.3.1",
    "gray-matter": "^4.0.3",
    "html-react-parser": "^1.2.7",
    "moment": "2.29.1",
    "netlify-cms-app": "github:PartlyFluked/netlify-cms#feature/usable-cloudflare-pages-app",
    "next": "11.0.1",
    "next-compose-plugins": "^2.2.1",
    "next-images": "1.8.1",
    "next-optimized-images": "^3.0.0-canary.10",
    "next-transpile-modules": "^8.0.0",
    "nextjs-redirect": "5.0.2",
    "nouislider": "15.2.0",
    "path": "0.12.7",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-datetime": "3.0.4",
    "react-dom": "17.0.2",
    "react-slick": "0.28.1",
    "remark": "^14.0.1",
    "remark-html": "^14.0.0",
    "remark-preset-lint-markdown-style-guide": "^4.0.0",
    "sass": "^1.37.5",
    "styled-components": "5.3.0",
    "superjson": "^1.7.5",
    "vfile-reporter": "^7.0.1",
    "webpack": "5.48.0"
  },
  "devDependencies": {
    "@types/node": "^16.4.13",
    "@types/react": "^17.0.16",
    "@types/react-dom": "^17.0.9",
    "@types/react-slick": "^0.23.5",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "npm-check-updates": "^11.8.3",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
